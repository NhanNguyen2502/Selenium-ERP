<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="All Test Suite">
    <listeners>
        <listener class-name="erp.base.ReportListener"></listener>
    </listeners>
    <test verbose="2" preserve-order="true" name="ERP_Selenium">
        <parameter name="language" value="English"></parameter>
        <classes>
            <class name="erp.testcases.SignIn.SignInTest"></class>
            <class name="erp.testcases.CompanyList.CompanyListTest"></class>
            <class name="erp.testcases.CreateCompany.CreateCompanyTest"></class>
            <class name="erp.testcases.Dashboard.DashboardTest"></class>
            <class name="erp.testcases.Sales.SalesTest"></class>
            <class name="erp.testcases.CreateSale.CreateSaleTest"></class>
        </classes>
    </test>
</suite>